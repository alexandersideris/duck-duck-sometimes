{
   "background": {
      "scripts": [ "background.js", "jquery-3.3.1.min.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content_script.js" ],
      "matches": ["*://*.google.com/*"],
      "run_at": "document_end"
   } ],
   "browser_action": {
      "default_icon": "images/private.png",
      "default_title": "Duck Duck Sometimes",
      "default_popup": "popup.html"
   },
   "description": "asd",
   "icons": { 
      "32": "images/private.png",
      "48": "images/private.png",
      "128": "images/private.png"
   },
   "manifest_version": 2,
   "name": "DuckDuckSometimes",
   "short_name": "DuckDuckSometimes",
   "permissions": [ "webRequest", "storage" , "tabs", "alarms", "notifications" ],
   "version": "1.0.41"
}